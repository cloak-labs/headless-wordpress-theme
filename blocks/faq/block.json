{
    "name": "acf/faq",
    "title": "FAQs",
    "description": "A custom block for displaying a list of FAQs.",
    "category": "widgets",
    "icon": "format-chat",
    "keywords": ["faqs", "frequently", "questions", "q&a", "answers"],
    "acf": {
        "mode": "edit",
        "renderTemplate": "block.php"
    },
    "align": false,
    "supports": {
        "mode": false,
        "align": false,
        "spacing": {
            "margin": ["top", "bottom"],
            "padding": ["top", "bottom"]
        },
        "color": {
            "background": true,
            "gradients": false,
            "text": false
        }
    },
    "attributes": {
        "style": {
            "type": "object",
            "default": {
                "spacing": {
                    "margin": {
                        "top": "80px",
                        "bottom": "80px"
                    },
                    "padding": {
                        "top": "0px",
                        "bottom": "0px"
                    }
                }
            }
        }
    }
}